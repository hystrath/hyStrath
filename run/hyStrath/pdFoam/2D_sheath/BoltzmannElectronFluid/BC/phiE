FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      phiE;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
        Inlet
	{
		type            fixedValue;
		value           uniform 0;
	}
	periodic_1
	{
		type            cyclic;
	}
	periodic_2
	{
		type            cyclic;
	}
	Wall
	{
		type            fixedValue;
		value 		uniform -2.11;
	}
	front
	{
		type            empty;
	}
	back
	{
	 	type            empty;
	}
}


