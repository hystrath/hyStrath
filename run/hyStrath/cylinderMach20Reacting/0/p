FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0.89;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.89;
    }
    cylinder
    {
        type            zeroGradient;
    }
    axis
    {
        type            symmetry;
    }
    outlet
    {
        type            zeroGradient;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
}


