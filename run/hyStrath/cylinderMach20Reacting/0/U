FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (6047 0 0);
    }
    cylinder
    {
        type            nonEqMaxwellSlipU;
        accommodationCoeff 1;
        Uwall           (0 0 0);
        thermalCreep    true;
        curvature       true;
        value           uniform (0 0 0);
    }
    axis
    {
        type            symmetry;
    }
    outlet
    {
        type            zeroGradient;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
}


