application     hy2Foam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         1.0e-1;

deltaT          1.0e-10;

writeControl    adjustableRunTime;

writeInterval   1e-6;

purgeWrite      30;

writeFormat     ascii;

writePrecision  10;

writeCompression off;

timeFormat      general;

timePrecision   10;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           0.1;

maxDeltaT       2.0e-5;

