FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      planeSurfacesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

surfaces
(
    zone
    {
        zoneName        face1;
        startPoint      (1.499e-6 0 0);
        endPoint        (1.501e-6 0 0);
        writeFaceSet    no;
    }
);


