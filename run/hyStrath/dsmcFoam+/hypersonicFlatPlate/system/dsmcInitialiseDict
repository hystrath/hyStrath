FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      dsmcInitialiseDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

configurations
(
    configuration
    {
        type                dsmcMeshFill;

        numberDensities
        {
            N2          0;
		    };

		    translationalTemperature     	300;
		    rotationalTemperature			300;
		    vibrationalTemperature			0;
        electronicTemperature           0;

		    velocity        (0 0 0);
    } 
);

