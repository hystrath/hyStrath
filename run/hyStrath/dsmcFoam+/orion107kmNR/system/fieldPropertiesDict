FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fieldPropertiesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dsmcFields
(
     field
     {
         fieldModel          	dsmcVolFields;
 
         timeProperties
         {
         	  timeOption          write;
 	    	    resetAtOutput		on;
 	    	    resetAtOutputUntilTime       1.4e-2;
         }
 
         dsmcVolFieldsProperties
         {
             fieldName               N2;
             typeIds                 (N2);
             measureMeanFreePath         true;
           	 averagingAcrossManyRuns     false;
         }
     }

     field
     {
         fieldModel          	dsmcVolFields;
 
         timeProperties
         {
         	  timeOption          write;
 	    	    resetAtOutput		on;
 	    	    resetAtOutputUntilTime       1.4e-2;
         }
 
         dsmcVolFieldsProperties
         {
             fieldName               O2;
             typeIds                 (O2);
             measureMeanFreePath         true;
           	 averagingAcrossManyRuns     false;
         }
     }

     
     field
     {
         fieldModel          	dsmcVolFields;
 
         timeProperties
         {
         	  timeOption          write;
 	    	    resetAtOutput		on;
 	    	    resetAtOutputUntilTime       1.4e-2;
         }
 
         dsmcVolFieldsProperties
         {
             fieldName               mixture;
             typeIds                 (N2 O2);
             measureMeanFreePath         true;
           	 averagingAcrossManyRuns     false;
         }
     }
);

