FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      probes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

probes
{
    type            probes;
    functionObjectLibs ("libsampling.so");
    enabled         true;
    outputControl   timeStep;
    outputInterval  1;

    fields
    (
        p
        rho
        
        N2
        O2
        NO
        N
        O
        N2+
        O2+
        NO+
        N+
        O+
        e-
        
        Tt
        Tv
        Tov
        Tv_N2
        Tv_O2
        Tv_NO
        Tv_N
        Tv_O
        Tv_e-
        
        nD_N2
        nD_O2
        nD_NO
        nD_N
        nD_O
        nD_N2+
        nD_O2+
        nD_NO+
        nD_N+
        nD_O+
        nD_e-
    );

    probeLocations
    (
        (5e-6 5e-6 5e-6)
    );

}
