FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.003";
    object      N2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];


internalField   uniform 0.7;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.7;
    }
    object
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
}


